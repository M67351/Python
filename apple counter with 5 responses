appnostr=input("how many apple? ")
appnoint=int((appnostr))
print(appnoint,"apples")
if appnoint == 0:
    print("haha")
elif appnoint == 1:
    print("not enough")
elif appnoint == 2:
    print("yay")
elif appnoint == 3:
    print("bit too much")
elif appnoint==4:
    print("why")
else:
    print("u got moneyyy")
